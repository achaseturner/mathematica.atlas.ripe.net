Cell[TextData[{
 "Conversion of Unix Machine Timestamp to ",
 StyleBox["HST",
  FontSlant->"Italic"]
}], "Subsubsection"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ClearAll", "[", "unixTimeGMTtoHST", "]"}], ";"}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"Hawaiian", " ", "Timezone", " ", "is"}], " ", "-", 
    RowBox[{"10", " ", "from", " ", "GMT"}]}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{" ", 
       RowBox[{"FIXME", " ", ":", " ", 
        RowBox[{"Old", " ", "conversion", " ", "strategy", " ", "was", " ", 
         RowBox[{"DateList", "[", 
          RowBox[{
           RowBox[{"AbsoluteTime", "[", 
            RowBox[{"{", 
             RowBox[{
             "1970", ",", "1", ",", "1", ",", "0", ",", "0", ",", "0"}], 
             "}"}], "]"}], "+", "x"}], "]"}]}]}], "]"}], ".", "  ", 
      "\[IndentingNewLine]", "Now"}], " ", "it", " ", "is", " ", "best", " ", 
     "to", " ", "use", " ", "FromUnixTime"}], ",", " ", 
    RowBox[{
    "except", " ", "it", " ", "does", " ", "not", " ", "yield", " ", "a", " ",
      "correct", " ", "timzone", " ", 
     RowBox[{"conversion", "."}]}]}], "  ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Clear", "[", "unixTimeGMTtoHST", "]"}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"NOTE", ":", " ", 
     RowBox[{
      RowBox[{"I", " ", "have", " ", "to", " ", "over"}], "-", 
      RowBox[{
      "ride", " ", "the", " ", "global", " ", "symbol", " ", "$TimeZone", " ",
        "to", " ", "suggest", " ", 
       RowBox[{"I", "'"}], "m", " ", "currently", " ", "in", " ", "the", " ", 
       "HST"}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"unixTimeGMTtoHST", "[", "x_Integer", "]"}], " ", ":=", 
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"$TimeZone", "=", 
         RowBox[{"-", "10"}]}], "}"}], ",", 
       RowBox[{"DateList", "[", 
        RowBox[{"FromUnixTime", "[", "x", "]"}], "]"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "OR", " ", "I", " ", "can", " ", "ensure", " ", "DataList", " ", "is", 
     " ", "called", " ", "with", " ", "the", " ", "correct", " ", 
     "timezone"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"unixTimeGMTtoHST", "[", "x_Integer", "]"}], " ", ":=", 
     RowBox[{"DateList", "[", 
      RowBox[{
       RowBox[{"FromUnixTime", "[", "x", "]"}], ",", 
       RowBox[{"TimeZone", "\[Rule]", 
        RowBox[{"-", "10"}]}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", "TEST", " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"Block", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", " ", 
      RowBox[{
       RowBox[{"testDateGMTAsUnixTime", "=", 
        RowBox[{"UnixTime", "[", 
         RowBox[{"DateObject", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
            "2015", ",", "1", ",", "1", ",", "0", ",", "0", ",", "0.0"}], 
            "}"}], ",", 
           RowBox[{"TimeZone", "\[Rule]", "0"}]}], "]"}], "]"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"testDateHST", " ", "=", " ", 
        RowBox[{"{", 
         RowBox[{
         "2014", ",", "12", ",", "31", ",", "14", ",", "0", ",", "0.0"}], 
         "}"}]}], " ", ",", "\[IndentingNewLine]", "testConversion", ",", 
       "\[IndentingNewLine]", "testResult"}], " ", "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"testConversion", "=", 
       RowBox[{"unixTimeGMTtoHST", "[", "testDateGMTAsUnixTime", "]"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "The", " ", "expected", " ", "output", " ", "for", " ", "HST", " ", 
        "is", " ", 
        RowBox[{"{", 
         RowBox[{
         "2014", ",", "12", ",", "31", ",", "14", ",", "0", ",", "0.0"}], 
         "}"}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"testResult", " ", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"testConversion", "===", "testDateHST"}], ",", 
         "\"\< == \>\"", ",", "\"\< != \>\""}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"\"\<convertedHST[\>\"", "<>", 
       RowBox[{"ToString", "[", "testConversion", "]"}], "<>", " ", 
       "testResult", " ", "<>", " ", "\"\<expected HST value:\>\"", "<>", " ", 
       RowBox[{"ToString", "[", "testDateHST", "]"}]}]}]}], 
    "\[IndentingNewLine]", "]"}]}]}]], "Input"]
