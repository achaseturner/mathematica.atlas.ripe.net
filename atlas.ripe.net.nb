Cell["\<\
rttDatum - convienience function to fetch a slice of rttDatum of interest.  \
\>", "Subsubsection"],

Cell["\<\
NOTE : rttDatum is cached and so, if you want to ensure it is re-set, issue \
the Clear[rttDatum] to force the reload.
NOTE: ALSO that I am caching based upon YYYY, MM, DD only.\
\>", "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "rttDatum", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"rttDatum", "[", 
    RowBox[{
    "probeID_", ",", "hop_", ",", "resolution_", ",", "startTime_", ",", " ", 
     "endTime_", ",", "rttMeasurementType_String"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"rttDatum", "[", 
     RowBox[{"probeID", ",", "hop", ",", "resolution", ",", 
      RowBox[{"startTime", "[", 
       RowBox[{"[", 
        RowBox[{"1", ";;", "3"}], "]"}], "]"}], ",", " ", 
      RowBox[{"endTime", "[", 
       RowBox[{"[", 
        RowBox[{"1", ";;", "3"}], "]"}], "]"}], ",", "rttMeasurementType"}], 
     "]"}], "=", 
    RowBox[{"Block", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"rttMeasurements", " ", "=", 
         RowBox[{"(", " ", 
          RowBox[{"\"\<measurements\>\"", "/.", 
           RowBox[{"(", " ", 
            RowBox[{"\"\<data\>\"", "/.", 
             RowBox[{"Import", "[", 
              RowBox[{"builtInFilePath", "[", 
               RowBox[{
               "probeID", ",", "hop", ",", " ", "resolution", ",", 
                "startTime", ",", "endTime"}], "]"}], "]"}]}], " ", ")"}]}], 
          ")"}]}], ",", "\[IndentingNewLine]", "rttSlice"}], 
       "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "Traverse", " ", "the", " ", "JASON", " ", "structure", " ", "to", " ",
         "reach", " ", "the", " ", "slices", " ", "of", " ", "interest"}], 
       " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"rttSlice", "=", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"unixTimeGMTtoHST", "[", "\"\<timestamp\>\"", "]"}], ",", 
           "rttMeasurementType"}], "}"}], "/.", "rttMeasurements"}]}], ";", 
       "\[IndentingNewLine]", "rttSlice"}]}], "\[IndentingNewLine]", 
     "]"}]}]}], ";"}]}], "Input"]
